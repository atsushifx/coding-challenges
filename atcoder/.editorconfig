# EditorConfig is awesome: https://EditorConfig.org
#
# @(#) : default settings for coding Challeng
#

# top-most EditorConfig file
##
root = false

# Unix-style newlines with a newline ending every file
# typescript
[**.ts]
tab_width = 2
indent_size = 2
indent_style = space

# Ruby
[**.rb]
tab_width = 2
indent_size = 2
indent_style = space

# go
[**.go]
indent_style = tab
tab_width = 4
indent_size = tab

# Racket
[**.{scm,rkt}]
indent_style = space
tab_width = 2
indent_size = 2
